{
  "name": "chrome-markdown",
  "version": "1.2.7",
  "description": "Preview GitHub Flavored Markdown in Chrome",
  "main": "dist/manifest.json",
  "scripts": {
    "lint": "standard",
    "build": "gulp build",
    "archive": "gulp archive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/chrome-markdown.git"
  },
  "keywords": [
    "chrome",
    "markdown",
    "markdown preview"
  ],
  "author": "zce <w@zce.me> (https://zce.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zce/chrome-markdown/issues"
  },
  "homepage": "https://github.com/zce/chrome-markdown#readme",
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.10.0",
    "gulp-uglify": "^3.0.1",
    "gulp-zip": "^4.2.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "src/js/highlight.js",
      "src/js/marked.js"
    ],
    "globals": [
      "chrome",
      "XMLHttpRequest"
    ]
  }
}
